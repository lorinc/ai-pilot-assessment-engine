      "typical_improvement_opportunities": {
        "team_execution": "Hire FP&A analysts, improve cross-functional collaboration",
        "system_capabilities": "Predictive analytics, driver-based planning, scenario modeling",
        "process_maturity": "Standardize assumptions, implement rolling forecasts"
