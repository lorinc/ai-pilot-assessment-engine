        "team": "Accounting",
        "process": "Financial Close Process",
        "step": "Reporting",
        "system": "ERP"
      },
      "typical_dependencies": ["journal_entries", "account_reconciliations", "transaction_data"],
      "common_pain_points": [
        "Close takes too long (10+ days)",
        "Errors discovered after close",
        "Manual consolidation across entities",
        "Lack of real-time visibility",
        "Audit findings and restatements"
      ],
      "typical_improvement_opportunities": {
        "team_execution": "Train on close procedures, hire senior accountants",
        "system_capabilities": "Automated consolidation, continuous close, AI anomaly detection",
        "process_maturity": "Standardize close checklist, implement continuous accounting"
      }
    },
    {
      "id": "financial_forecasts",
