{
  "function": "Finance",
  "description": "Financial planning, reporting, and control",
  
  "typical_teams": [
    {
      "name": "Financial Planning & Analysis (FP&A)",
      "roles": ["FP&A Analyst", "Financial Analyst", "Budget Manager"],
      "outputs_created": ["financial_forecasts", "budget_plans", "variance_reports", "board_reports"]
    },
    {
      "name": "Accounting",
      "roles": ["Accountant", "Controller", "Accounting Manager"],
      "outputs_created": ["financial_statements", "journal_entries", "account_reconciliations", "audit_reports"]
    },
    {
      "name": "Accounts Payable",
      "roles": ["AP Specialist", "AP Manager"],
      "outputs_created": ["vendor_payments", "expense_reports", "payment_schedules"]
    },
    {
      "name": "Accounts Receivable",
      "roles": ["AR Specialist", "Collections Manager"],
      "outputs_created": ["invoices", "payment_collections", "aging_reports"]
    },
    {
      "name": "Treasury",
      "roles": ["Treasurer", "Cash Manager"],
      "outputs_created": ["cash_flow_forecasts", "liquidity_reports", "investment_decisions"]
    }
  ],
  
  "typical_processes": [
    {
      "name": "Financial Close Process",
      "description": "Month-end/quarter-end/year-end financial closing",
      "steps": ["Transaction Cutoff", "Reconciliations", "Adjustments", "Review", "Reporting"],
      "outputs_by_step": {
        "Reconciliations": ["account_reconciliations"],
        "Adjustments": ["journal_entries"],
        "Reporting": ["financial_statements", "management_reports"]
      }
    },
    {
      "name": "Budgeting & Planning Process",
      "description": "Annual budget creation and periodic forecasting",
      "steps": ["Guidelines", "Department Submissions", "Consolidation", "Review & Approval", "Distribution"],
      "outputs_by_step": {
        "Department Submissions": ["department_budgets"],
        "Consolidation": ["consolidated_budget", "budget_plan"],
        "Distribution": ["approved_budget"]
      }
    },
    {
      "name": "Accounts Payable Process",
      "description": "Processing vendor invoices and payments",
      "steps": ["Invoice Receipt", "Validation", "Approval", "Payment Processing", "Reconciliation"],
      "outputs_by_step": {
        "Validation": ["validated_invoices"],
        "Payment Processing": ["vendor_payments", "payment_files"],
        "Reconciliation": ["payment_reconciliation"]
      }
    },
    {
      "name": "Financial Forecasting Process",
      "description": "Updating financial projections based on actuals",
      "steps": ["Data Collection", "Assumption Updates", "Model Updates", "Review", "Distribution"],
      "outputs_by_step": {
        "Model Updates": ["financial_forecast"],
        "Distribution": ["forecast_presentation"]
      }
    }
  ],
  
  "typical_systems": [
    {
      "category": "ERP",
      "examples": ["SAP", "Oracle Financials", "NetSuite", "Microsoft Dynamics", "Workday"],
      "outputs_created": ["financial_statements", "journal_entries", "account_reconciliations", "vendor_payments"]
    },
    {
      "category": "Financial Planning",
      "examples": ["Anaplan", "Adaptive Insights", "Planful", "Vena"],
      "outputs_created": ["budget_plans", "financial_forecasts", "variance_reports", "scenario_models"]
    },
    {
      "category": "Expense Management",
      "examples": ["Concur", "Expensify", "Coupa"],
      "outputs_created": ["expense_reports", "reimbursements", "spend_analytics"]
    },
    {
      "category": "Spreadsheets",
      "examples": ["Excel", "Google Sheets"],
